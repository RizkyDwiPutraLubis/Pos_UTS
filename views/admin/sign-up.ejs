


<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
      <title><%= title %></title>
      
      <!-- Favicon -->
      <link rel="shortcut icon" href="<%= baseUrl %>/assets/img/LogoPos.jpeg" type="image/x-icon">
      <link rel="stylesheet" href="<%= baseUrl %>/assets_admin/css/backend-plugin.min.css">
      <link rel="stylesheet" href="<%= baseUrl %>/assets_admin/css/backend.css?v=1.0.0">
      <link rel="stylesheet" href="<%= baseUrl %>/assets_admin/vendor/@fortawesome/fontawesome-free/css/all.min.css">
      <link rel="stylesheet" href="<%= baseUrl %>/assets_admin/vendor/line-awesome/dist/line-awesome/css/line-awesome.min.css">
      <link rel="stylesheet" href="<%= baseUrl %>/assets_admin/vendor/remixicon/fonts/remixicon.css">  
   <style>
      
   </style>
   </head>
  <body class=" ">
    
      <div class="wrapper">
      <section class="login-content">
         <div class="container">
            <div class="row align-items-center justify-content-center height-self-center">
               <div class="col-lg-8">
                  <div class="card auth-card">
                     <div class="card-body p-0">
                        <div class="d-flex align-items-center auth-content">
                           <div class="col-lg-7 align-self-center">
                             
                              <div class="p-3">
                                 <% if(messages.length !== 0 ){ %>
                                    <div class="alert alert-danger">
                                      <strong><%= messages %> </strong>
                                      <button type="button" class="btn-close btn-close-white ml-2" data-dismiss="alert" aria-label="Close">
                                       <span aria-hidden="true">&times;</span>
                                     </button> 
                                       
                                    </div>
                                  <% } %> 
                                 <h2 class="mb-1">Sign Up</h2>
                                 <p>Create your POS RIZKY account.</p>
                                 <form  action="/register" method="post">
                                    <div class="row">
                                       <div class="col-lg-6">
                                          <div class="floating-label form-group">
                                             <input class="floating-input form-control" type="email" name="email" required placeholder="">
                                       
                                             <label>Email</label>
                                          </div>
                                       </div>

                                       <div class="col-lg-6">
                                          <div class="floating-label form-group">
                                             <input class="floating-input form-control" type="text" name="fullname" value="" required placeholder="">
                                             <label>Full Name</label>
                                          </div>
                                       </div>
                                       
                                      
                                       
                                       <div class="col-lg-6">
                                          <div class="floating-label form-group">
                                             <input class="floating-input form-control" type="password" name="password" required id="password" placeholder="">
                                             <label>Password</label>
                                          </div>
                                       </div>
                                       <div class="col-lg-6">
                                          <div class="floating-label form-group">
                                         
                                             <input class="floating-input form-control" type="password" name="password_confirmation" required id="password_confirmation" placeholder="">
                                             <label>Confirm Password</label>
                                          </div>
                                       </div>
                                     
                                    </div>
                                    <button type="submit" class="btn btn-primary">Sign Up</button>
                                    <p class="mt-3">
                                       Already have an Account <a href="<%= baseUrl %>/login" class="text-primary">Sign In</a>
                                    </p>
                                 </form>
                              </div>
                           </div>
                           <div class="col-lg-5 content-right">
                              <img src="<%= baseUrl %>/assets_admin/images/login/01.png" class="img-fluid image-right" alt="">
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </section>
      </div>
    
    <!-- Backend Bundle JavaScript -->
    <script src="<%= baseUrl %>/assets_admin/js/backend-bundle.min.js"></script>
    
    <!-- Table Treeview JavaScript -->
    <script src="<%= baseUrl %>/assets_admin/js/table-treeview.js"></script>
    
    <!-- Chart Custom JavaScript -->
    <script src="<%= baseUrl %>/assets_admin/js/customizer.js"></script>
    
    <!-- Chart Custom JavaScript -->
    <script async src="<%= baseUrl %>/assets_admin/js/chart-custom.js"></script>
    
    <!-- app JavaScript -->
    <script src="<%= baseUrl %>/assets_admin/js/app.js"></script>
    <script>
      // Sembunyikan alert setelah 5 detik
      window.addEventListener('load', () => {
        const alert = document.querySelector('.alert');
        if (alert) {
          setTimeout(() => {
            alert.classList.add('d-none'); // Sembunyikan dengan kelas d-none (Bootstrap)
          }, 5000);
        }
      });
    </script>
  </body>
</html>